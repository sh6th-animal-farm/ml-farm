<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.animalfarm.mlf.domain.accounting.RevenueRepository">
	<update id="updateSummaryId">
	    UPDATE revenues
	    SET rs_id = #{rsId}
	    WHERE project_id = #{projectId}
	      AND rs_id IS NULL
	</update>
</mapper>